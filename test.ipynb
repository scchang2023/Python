   "source": [
    "def my_range(first=0, last=0, step=1):\n",
    "    number = first\n",
    "    while number < last:\n",
    "        yield number\n",
    "        number+=step\n",
    "# 它是個一般函式\n",
    "print(my_range)\n",
    "# 回傳一個產生器物件\n",
    "ranger = my_range(1, 5)\n",
    "print(ranger)\n",
    "# 可以迭代這個產生器物件\n",
    "for x in ranger:\n",
    "    print(x)"
   ]
  }
